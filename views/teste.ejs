<style>
  .scu-grid{
    width: 80%;
    columns: 5 200px;
    column-gap: 1rem;
  }

  .scu-grid .item{
    width: 150px;
    color: white;
    margin: 0 1rem 1rem 0;
    display: inline-block;
    width: 100%;
  }

  .scu-grid .item > img{
    position: relative;
    width: 100%;
  }
</style>
<div class="scu-grid"></div>
<script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
<script type="text/javascript">
  $(document).ready(() => {
    $.get("https://hawkhomolog.herokuapp.com/sku-pictures-page?sku=CB.V007am&page=1", function( data ) {
      data.forEach((each)=>{

        $('.scu-grid').append($('<div>').addClass('item').append($('<img>').attr('src', each.url)));
        });
      });
    });
  </script>
