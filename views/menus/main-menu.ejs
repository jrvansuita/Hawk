<html>
  <head>
    <link rel="stylesheet" type="text/css" href="/front/css/layout/menu.css" />
    <link rel="stylesheet" type="text/css" href="/front/css/layout/general.css" />
    <script type="text/javascript" src="/front/js/libs/menu-controller.js"></script>
    <script type="text/javascript" src="/util/local.js"></script>
    <script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
    <script type="text/javascript" src="/front/js/control/menu.js"></script>
    <script type="text/javascript" src="/util/string.js"></script>
    <script type="text/javascript" src="/util/date.js"></script>
    <script type="text/javascript" src="/util/number.js"></script>
    <script type="text/javascript" src="/util/util.js"></script>
    <script type="text/javascript" src="/util/array.js"></script>
    <script type="text/javascript" src="/util/float.js"></script>
    <script type="text/javascript" src="/util/sett.js"></script>
    <script type="text/javascript" src="/param"></script>
    <script type="text/javascript" src="/front/js/control/general.js"></script>
    <script type="text/javascript" src="/front/js/libs/file-loader.js"></script>

    <link href="//fonts.googleapis.com/css?family=Roboto" rel="stylesheet" type="text/css" />

    <title>Boutique Infantil</title>
    <link rel="icon" href="/img/icon/favicon.ico" type="image/x-icon" />
    <link rel="shortcut icon" href="/img/icon/favicon.ico" type="image/x-icon" />
    <link rel="icon" type="image/png" sizes="32x32" href="/img/icon/favicon-32x32.png" />
    <link rel="icon" type="image/png" sizes="96x96" href="/img/icon/favicon-96x96.png" />
    <link rel="icon" type="image/png" sizes="16x16" href="/img/icon/favicon-16x16.png" />
  </head>

  <body>
    <script type="text/javascript" class="hidable-script">
      window.loggedUser = <%- JSON.stringify(loggedUser || null) %>
      window._params = <%- JSON.stringify(Params.export() || null) %>
    </script>

    <nav class="main-menu-nav">
      <div class="start-jobs">
        <% var img = '/img/icon/bi.png'; if (!process.env.IS_PRODUCTION){ img = '/img/maintenance.png'; } if (global.eccoConnErrors){ img = '/img/api-error.png'; } %>
        <img src="<%=img%>" id="logo" title="<%=global.eccoConnError? 'Erro ao conectar e receber dados via API' : 'Boutique Infantil' %>" />
      </div>

      <ul class="main-menu-ul">
        <%if (Menu.has(loggedUser, 'picking')){%>
        <li class="menu-item" sub="picking-menu">
          <a href="#">
            <div class="menu-button title">
              <span class="menu-label">Picking</span>
            </div>
          </a>
        </li>
        <%}%> <%if (Menu.has(loggedUser, 'packing')){%>
        <li class="menu-item" sub="packing-menu">
          <a href="#">
            <div class="menu-button title">
              <span class="menu-label">Packing</span>
            </div>
          </a>
        </li>
        <%}%> <%if (Menu.has(loggedUser, 'pendência')){%> <%if (Sett.get(locals.loggedUser,2)){%>
        <li class="menu-item" sub="pending-menu">
          <a href="#">
            <div class="menu-button title">
              <span class="menu-label">Pendência</span>
            </div>
          </a>
        </li>
        <%}%> <%}%> <%if (Menu.has(loggedUser, 'performance')){%>
        <li class="menu-item" sub="performance-menu">
          <a href="#">
            <div class="menu-button title">
              <span class="menu-label">Performance</span>
            </div>
          </a>
        </li>
        <%}%> <%if (Menu.has(loggedUser, 'atendimento')){%>
        <li class="menu-item" sub="customer-menu">
          <a href="#">
            <div class="menu-button title">
              <span class="menu-label">Atendimento</span>
            </div>
          </a>
        </li>
        <%}%> <%if (Menu.has(loggedUser, 'estoque')){%>
        <li class="menu-item" sub="stock-menu">
          <a href="#">
            <div class="menu-button title">
              <span class="menu-label">Estoque</span>
            </div>
          </a>
        </li>
        <%}%> <%if (Menu.has(loggedUser, 'histórico')){%>
        <li class="menu-item fixed-menu" sub="history-menu">
          <a href="/history">
            <div class="menu-button title">
              <span class="menu-label">Histórico</span>
            </div>
          </a>
        </li>
        <%}%> <%if (Menu.has(loggedUser, 'Configurações')){%>
        <li class="menu-item fixed-menu" sub="settings-menu">
          <a href="/settings">
            <div class="menu-button title">
              <span class="menu-label">Configurações</span>
            </div>
          </a>
        </li>
        <%}%>
      </ul>

      <div class="user-logged-holder">
        <img src="<%=loggedUser ? loggedUser.avatar : 'default' %>" class="user-logged-img circle shadow" onerror="this.src='/img/avatar.png'" />

        <label class="title user-logged-name"><%= loggedUser ? loggedUser.name : 'No User' %> </label>
      </div>
    </nav>
  </body>
</html>
