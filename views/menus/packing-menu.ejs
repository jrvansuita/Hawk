<div class="sub-menu">
  <nav class="sub-menu-nav">
    <ul class="sub-menu-ul">
      <%if (Sett.get(locals.loggedUser,8)){%>
        <li class="menu-item material-ripple" href="/packing">
          <div class="menu-button title">
            Em Aberto
          </div>
        </li>
      <%}%>
      <li class="menu-item material-ripple" href="/packing/overview">
        <div class="menu-button title">
          Geral
        </div>
      </li>
      <li class="menu-item material-ripple" href="/packing/by-date">
        <div class="menu-button title">
