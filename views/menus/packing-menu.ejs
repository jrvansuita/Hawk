  <div class="subtitles">

    <%
    if (typeof subtitlesArr != "undefined")
      subtitlesArr.forEach(function(bar){ %>

      <div class="subtitle-item">
        <div class="subtitle-color circle shadow" style="background-color: #<%=bar.barColor%>"> </div>
        <label class="subtitle-label"> <%=bar.name%> </label>
      </div>

      <% }); %>

  </div>
</div>
